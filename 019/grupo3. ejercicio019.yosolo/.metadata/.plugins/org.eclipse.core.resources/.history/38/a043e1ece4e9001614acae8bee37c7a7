package es.cic.curso.curso14.ejercicio016.dominio;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

import es.cic.curso.curso14.ejercicio016.repositorioEstatico.Identificable;
@Entity
public class Salas implements Identificable<Long>{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private Long id;
	
	@Column(name="fila")
	private int fila;

	public Salas() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Salas(long salaId, int fila) {
		super();
		this.fila = fila;
	}

	public Salas(Long id, long salaId, int fila) {
		super();
		this.id = id;
		this.fila = fila;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public int getFila() {
		return fila;
	}

	public void setFila(int fila) {
		this.fila = fila;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + fila;
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Salas other = (Salas) obj;
		if (fila != other.fila)
			return false;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;

		return true;
	}

	@Override
	public String toString() {
		return "CineSalas [id=" + id + ", fila=" + fila + "]";
	}
	
	
	
	
}
