package es.cic.curso.curso14.ejercicio016.dominio;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

import es.cic.curso.curso14.ejercicio016.repositorioEstatico.Identificable;
@Entity
public class Reservas implements Identificable<Long> {


	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private Long id;
	
	@Column(name="asientoid")
	private long asientoid;
	
	@Column(name="estado")
	private boolean estado;

	public Reservas() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Reservas(boolean estado, long asientoid) {
		super();

		this.asientoid = asientoid;
		this.estado = estado;
	}

	public Reservas(Long id, boolean estado, long asientoid) {
		super();
		this.id = id;

		this.asientoid = asientoid;
		this.estado = estado;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + (int) (asientoid ^ (asientoid >>> 32));
		result = prime * result + (estado ? 1231 : 1237);
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Reservas other = (Reservas) obj;
		if (asientoid != other.asientoid)
			return false;
		if (estado != other.estado)
			return false;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;
		return true;
	}

	@Override
	public String toString() {
		return "CineReserva [id=" + id + ", asientoid=" + asientoid + ", estado=" + estado
				+ "]";
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}


	public long getAsientoid() {
		return asientoid;
	}

	public void setAsientoid(long asientoid) {
		this.asientoid = asientoid;
	}

	public boolean isEstado() {
		return estado;
	}

	public void setEstado(boolean estado) {
		this.estado = estado;
	}
	
	
	
	
	
	
	
}
