package es.cic.curso.curso14.ejercicio016.dominio;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.Table;

import es.cic.curso.curso14.ejercicio016.repository.Identificable;

@Entity
@Table(name="venta")
public class Venta implements Identificable<Long> {
	
	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private Long id;
	
	@Column(name="precio")
	private static double PRECIO = 5.0;

		
	@Column(name="sesionId")
	private long sesionId;
	
	@Column(name="salaId")
	private long salaId;
	
	@Column(name="recaudacionsala")
	private double recaudacionSala;
	
	@Column(name="cerrado")
	private boolean cerrado;

	public Venta(int i, int j, boolean b) {
		super();
	}

	public Venta(Long id, long salaId, long sesionId, double recaudacionSala, boolean cerrado) {
		super();
		this.id = id;
		this.salaId = salaId;
		this.sesionId = sesionId;
		this.recaudacionSala = recaudacionSala;
	}
	
	public Venta(long salaId, long sesionId, double recaudacionSala, boolean cerrado) {
		super();

		this.salaId = salaId;
		this.sesionId = sesionId;
		this.recaudacionSala = recaudacionSala;
	}
	

	@Override
	public Long getId() {
		return id;
	}

	@Override
	public void setId(Long id) {
		this.id = id;
	}


	public long getSesionId() {
		return sesionId;
	}

	public void setSesionId(long sesionId) {
		this.sesionId = sesionId;
	}

	public double getRecaudacionSala() {
		return recaudacionSala;
	}

	public void setRecaudacionSala(double recaudacionSala) {
		this.recaudacionSala = recaudacionSala;
	}

	public boolean isCerrado() {
		return cerrado;
	}

	public void setCerrado(boolean cerrado) {
		this.cerrado = cerrado;
	}

	public static double getPRECIO() {
		return PRECIO;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Venta other = (Venta) obj;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;
		return true;
	}
}