package es.cic.curso.curso14.ejercicio016.dominio;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

import es.cic.curso.curso14.ejercicio016.repositorioEstatico.Identificable;

@Entity
public class Asientos implements Identificable<Long>{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private Long id;

	@Column(name="salaId")
	private long salaId;
	
	@Column(name="fila")
	private int fila;
	
	@Column(name="precio")
	private static double PRECIO = 5.5;
	
	@Column(name="recaudacionsala")
	private double recaudacionSala;

	public Asientos() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Asientos(long salaId, int fila, double recaudacionSala) {
		super();
		this.salaId = salaId;
		this.fila = fila;
		this.recaudacionSala = recaudacionSala;
	}

	public Asientos(Long id, long salaId, int fila, double recaudacionSala) {
		super();
		this.id = id;
		this.salaId = salaId;
		this.fila = fila;
		this.recaudacionSala = recaudacionSala;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public long getSalaId() {
		return salaId;
	}

	public void setSalaId(long salaId) {
		this.salaId = salaId;
	}

	public int getFila() {
		return fila;
	}

	public void setFila(int fila) {
		this.fila = fila;
	}

	public double getRecaudacionSala() {
		return recaudacionSala;
	}

	public void setRecaudacionSala(double recaudacionSala) {
		this.recaudacionSala = recaudacionSala;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + fila;
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		long temp;
		temp = Double.doubleToLongBits(recaudacionSala);
		result = prime * result + (int) (temp ^ (temp >>> 32));
		result = prime * result + (int) (salaId ^ (salaId >>> 32));
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Asientos other = (Asientos) obj;
		if (fila != other.fila)
			return false;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;
		if (Double.doubleToLongBits(recaudacionSala) != Double.doubleToLongBits(other.recaudacionSala))
			return false;
		if (salaId != other.salaId)
			return false;
		return true;
	}

	@Override
	public String toString() {
		return "CineAsientos [id=" + id + ", salaId=" + salaId + ", fila=" + fila + ", recaudacionSala="
				+ recaudacionSala + "]";
	}
	
	
}
