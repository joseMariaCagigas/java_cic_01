package es.cic.curso.curso14.ejercicio016.dominio;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

import es.cic.curso.curso14.ejercicio016.repositorioEstatico.Identificable;
@Entity
public class Horarios implements Identificable<Long>{


	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private Long id;
	
	@Column(name="sesionId")
	private long sesionId;
	
	@Column(name="hora")
	private String hora;
	
	@Column(name="fecha")
	private String fecha;

	public Horarios() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Horarios(String hora, String fecha, long sesionId) {
		super();
		this.sesionId = sesionId;
		this.hora = hora;
		this.fecha = fecha;
	}

	public Horarios(Long id, String hora, String fecha, long sesionId) {
		super();
		this.id = id;
		this.sesionId = sesionId;
		this.hora = hora;
		this.fecha = fecha;
	}
	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}
	public String getHora() {
		return hora;
	}

	public void setHora(String hora) {
		this.hora = hora;
	}

	public String getFecha() {
		return fecha;
	}

	public void setFecha(String fecha) {
		this.fecha = fecha;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((fecha == null) ? 0 : fecha.hashCode());
		result = prime * result + ((hora == null) ? 0 : hora.hashCode());
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		result = prime * result + (int) (sesionId ^ (sesionId >>> 32));
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Horarios other = (Horarios) obj;
		if (fecha == null) {
			if (other.fecha != null)
				return false;
		} else if (!fecha.equals(other.fecha))
			return false;
		if (hora == null) {
			if (other.hora != null)
				return false;
		} else if (!hora.equals(other.hora))
			return false;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;
		if (sesionId != other.sesionId)
			return false;
		return true;
	}


	
	
}
