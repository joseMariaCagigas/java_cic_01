package es.cic.curso.curso14.ejercicio016.dominio;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

import es.cic.curso.curso14.ejercicio016.repositorioEstatico.Identificable;

@Entity
public class Asientos implements Identificable<Long>{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private Long id;

	@Column(name="salaId")
	private long salaId;
	

	@Column(name="precio")
	private static double PRECIO = 5.5;
	


	public Asientos() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Asientos(long salaId, int fila, double recaudacionSala) {
		super();
		this.salaId = salaId;

	}

	public Asientos(Long id, long salaId, int fila, double recaudacionSala) {
		super();
		this.id = id;
		this.salaId = salaId;

	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public long getSalaId() {
		return salaId;
	}

	public void setSalaId(long salaId) {
		this.salaId = salaId;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((id == null) ? 0 : id.hashCode());
		result = prime * result + (int) (salaId ^ (salaId >>> 32));
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Venta other = (Venta) obj;
		if (id == null) {
			if (other.id != null)
				return false;
		} else if (!id.equals(other.id))
			return false;
		if (salaId != other.salaId)
			return false;
		return true;
	}

	
	
}
